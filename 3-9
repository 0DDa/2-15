#include "iostream"
#include "string"
using namespace std;
int main()
{
char b;
cin >> b;
string str,slovo;
string::size_type k=0,pos=0;
cout<<"Enter str: "<<endl;
getline(cin,str);
str=str+"";
k=str.find(" ",pos);
while(k!=string::npos)
{ slovo=str.substr(pos, k-pos);
if (slovo[0]==b)
{
if (slovo[0]==slovo[slovo.length()-1]);
pos=k+1;
cout<<slovo<<endl;
k=str.find(" ",pos);
}
pos=k+1;
}
return 0;}

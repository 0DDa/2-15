#include <iostream>
#include <cmath>
using namespace std;
int main() {
	setlocale (LC_ALL, "Russian");
	int n;
	cout << "n = ";
	cin >> n;
	float x;
	cout << "x = ";
	cin >> x;
	float s, a=1./12;
	s = a;
	if (n >= 1) {
		cout << "s = " << a << endl;
	for (int i = 2; i < n; i++)
	{
		a *= -x/(3*i - 3);
		s += a;
		cout << "s = " << s << endl;
	}
	}
	return 0;
}

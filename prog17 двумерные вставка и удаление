#include <iostream>
using namespace std;
int main()
{
	int n, m;
	cout << "n = ";
	cin >> n;
	cout << "m = ";
	cin >> m;
	int **a = new int *[n];
	for (int i=0; i < n; i++)
	a[i] = new int [m];
	cout << "Enter array elements\n";
	for (int i = 0; i < n; i++)
		for (int j = 0; j < n; j++)
		{
			cout << "a[" << i << "][" << j << "] = ";
			cin >> a[i][j];
		}
	int s;
	for (int i = 0; i < n; i++)
	{
		s = 0;
		for (int j = 0; j < m; j++)
			s += a[i][j];
		if (s >= 10 && s <= 99)
		{
			for (int k = i; k < n - 1; k++)
				for (int j = 0; j < m; j++)
				a[k][j] = a[k+1][j];
			n--;
			i--;
		}
	}
	cout << "New array\n";
	for (int i = 0; i < n; i++, cout << endl)
		for (int j = 0; j < m; j++)
			cout << a[i][j] << "\t";
	return 0;
}

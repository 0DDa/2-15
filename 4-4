#include <iostream>
using namespace std;
int main()
{
    setlocale(LC_ALL, "Russian");
    int num, l;
    cout << "Количество элементов массива = ";
    cin >> num;
    cout << "Условие = ";
    cin >> l;
    int *mas = new int [num];
    for (int i = 0; i < num; ++i)
    {
    	cout << "mas[" << i << "]= ";
    	cin >> mas[i];
    }
    for(int i = 0; i < num ;i++)
    {
    	        if (abs(mas[i]%10) != l)
    	        {
    	        	cout << mas[i] << '\n';
    	        }
    }
    return 0;
}
